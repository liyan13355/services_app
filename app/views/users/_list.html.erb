<div id="filterrific_results">

  <div>
    <%= page_entries_info users %>
  </div>

  

  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Country</th>
      <th>Service Provided</th>
      <th>Registered at</th>
    </tr>
    <% users.each do |user| %>
      <tr>
        <td><%= link_to(user.full_name, user_path(user)) %></td>
        <td><%= user.email %></td>
        <td><%= user.country.name %></td>
        <td><%= user.service %></td>
        <td><%= user.decorated_created_at %></td>
      </tr>
    <% end %>
  </table>
</div>

<div>
<%= will_paginate users %>
</div>